{
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": 0
    },
    "overrides": [{
        "files": ["**/*.cjs"],
        "env": {
            "node": true
        }
    }]
}